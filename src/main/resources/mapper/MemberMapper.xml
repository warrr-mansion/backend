<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.warrr.zipflex.api.member.dao.MemberDao">

	<!-- member 단건 조회 -->
	<select id="findByUuid" resultType="com.warrr.zipflex.api.member.domain.entity.Member">
		SELECT member_uuid, nickname, email,
			password, state, role
		FROM member
		WHERE member_uuid = #{member_uuid}
	</select>
	
</mapper>